{
  "Rules": [
    {
      "ID": "BillingArchiveLifecycle",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "billing-archive/"
      },
      "Transitions": [
        {
          "Days": 90,
          "StorageClass": "GLACIER_IR"
        },
        {
          "Days": 365,
          "StorageClass": "DEEP_ARCHIVE"
        }
      ],
      "Expiration": {
        "Days": 3650
      }
    }
  ]
}
